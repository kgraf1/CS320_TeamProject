<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
  <display-name>ModelWebsiteRegistration</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
 
	<!-- Servlets -->
  <servlet>
    <servlet-name>ModelsByCategoryServlet</servlet-name>
    <servlet-class>servlets.ModelsByCategoryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModelsByKeywordServlet</servlet-name>
    <servlet-class>servlets.ModelsByKeywordsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModelsByMaterialServlet</servlet-name>
    <servlet-class>servlets.ModelsByMaterialServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModelsByProfileNameServlet</servlet-name>
    <servlet-class>servlets.ModelsByProfileNameServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModelsByTitleServlet</servlet-name>
    <servlet-class>servlets.ModelsByTitleServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddModelSubmissionServlet</servlet-name>
  	<servlet-class>servlets.AddModelSubmissionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<servlet-class>servlets.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProfileServlet</servlet-name>
  	<servlet-class>servlets.ProfileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayModelServlet</servlet-name>
  	<servlet-class>servlets.DisplayModelServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddRatingSubmissionServlet</servlet-name>
  	<servlet-class>servlets.AddRatingSubmissionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomepageServlet</servlet-name>
  	<servlet-class>servlets.HomepageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ModelsByProfileIdServlet</servlet-name>
  	<servlet-class>servlets.ModelsByProfileIdServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProfileOptionsServlet</servlet-name>
  	<servlet-class>servlets.ProfileOptionsServlet</servlet-class>
  </servlet>
  
  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>ModelsByCategoryServlet</servlet-name>
    <url-pattern>/ModelsByCategory</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ModelsByKeywordServlet</servlet-name>
    <url-pattern>/ModelsByKeyword</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ModelsByMaterialServlet</servlet-name>
    <url-pattern>/ModelsByMaterial</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ModelsByProfileNameServlet</servlet-name>
    <url-pattern>/ModelsByProfileName</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ModelsByTitleServlet</servlet-name>
    <url-pattern>/ModelsByTitle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddModelSubmissionServlet</servlet-name>
    <url-pattern>/addModelSubmission</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProfileServlet</servlet-name>
  	<url-pattern>/profilePage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayModelServlet</servlet-name>
  	<url-pattern>/DisplayModel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddRatingSubmissionServlet</servlet-name>
  	<url-pattern>/addRatingSubmission</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomepageServlet</servlet-name>
  	<url-pattern>/homepage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModelsByProfileIdServlet</servlet-name>
  	<url-pattern>/ModelsByProfileId</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProfileOptionsServlet</servlet-name>
  	<url-pattern>/ProfileOptions</url-pattern>
  </servlet-mapping>
  
</web-app>